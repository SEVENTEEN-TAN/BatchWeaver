<job id="multiStepJob">
    <!-- Step 1: 数据预处理 -->
    <step id="preprocessData" className="com.example.batch.steps.Step1Processor" methodName="execute"/>
    
    <!-- Step 2: 数据转换 -->
    <step id="transformData" className="com.example.batch.steps.Step2Processor" methodName="execute"/>
    
    <!-- Step 3: 数据加载 -->
    <step id="loadData" className="com.example.batch.steps.Step3Processor" methodName="execute"/>
</job>
